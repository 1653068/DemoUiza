<h4>All videos</h4>
<div class="container-fluid" id="inventory">
  <div class="row">
    {{#each entityList}}
    <div class="col-md-3">
      <div class="card">
        <img class="img-fluid" src="{{thumbnail}}" alt="Card image cap">
        <a href="/entity/{{id}}" class="btn view-btn">{{name}}</a>
        <div class="ribbon"><span>{{type}}</span></div>
      </div>
    </div>
    {{/each}}
  </div>
</div>


<div class="container" style="padding-bottom: 50px">
  {{{ paginate ./pagination leftText="<<" rightText=">>" limit="5"}}}
</div>